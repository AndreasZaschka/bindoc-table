{
  "name": "@bindoc/table",
  "version": "1.0.0-beta.2",
  "description": "Library QuickStart package.json from the documentation, supplemented with testing support",
  "main": "./table.umd.js",
  "module": "./table.es5.js",
  "es2015": "./table.js",
  "typings": "./table.d.ts",
  "author": "Sven Seemann",
  "bugs": {
    "url": "https://bindoc.atlassian.net/projects/BDMRGNCY/issues",
    "email": "sven.seemann@bindoc.de"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/bindoc/bindoc-table"
  },
  "publishConfig": {
    "registry": "https://npmjs.org"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "build-demo": "tsc -p src/demo/",
    "build-demo:watch": "tsc -p src/demo/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build-demo",
    "start": "concurrently \"npm run build-demo:watch\" \"npm run serve\"",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "pretest": "npm run build-test",
    "test": "concurrently \"npm run build-test:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build-test",
    "test:once": "karma start karma.conf.js --single-run",
    "preintegration": "npm run build && cd integration && npm run clean && npm install",
    "integration": "npm run integration:aot && npm run integration:jit",
    "integration:jit": "cd integration && npm run e2e",
    "integration:aot": "cd integration && npm run e2e:aot",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "peerDependencies": {
    "@angular/core": "4.4.x",
    "@angular/animations": "4.4.x",
    "@angular/forms": "4.4.x",
    "@angular/router": "4.4.x",
    "@angular/http": "4.4.x",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/material": "2.0.0-beta.12",
    "@angular/cdk": "2.0.0-beta.12",
    "@bindoc/templates": "~1.0.0-beta.2"
  },
  "devDependencies": {
    "@angular/animations": "4.4.x",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/common": "4.4.x",
    "@angular/compiler": "4.4.x",
    "@angular/compiler-cli": "4.4.x",
    "@angular/core": "4.4.x",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/forms": "4.4.x",
    "@angular/http": "4.4.x",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "4.4.x",
    "@angular/platform-browser-dynamic": "4.4.x",
    "@angular/router": "4.4.x",
    "@bindoc/templates": "~1.0.0-beta.2",
    "@types/jasmine": "2.5.x",
    "@types/node": "6.0.x",
    "camelcase": "4.1.x",
    "concurrently": "3.5.x",
    "core-js": "2.5.x",
    "glob": "7.1.x",
    "jasmine-core": "2.5.x",
    "lite-server": "2.2.x",
    "rimraf": "2.6.x",
    "rollup": "0.43.x",
    "rollup-plugin-sourcemaps": "0.4.x",
    "rollup-plugin-uglify": "2.0.x",
    "rxjs": "5.5.x",
    "systemjs": "0.20.x",
    "typescript": "2.4.x",
    "zone.js": "0.8.x"
  },
  "dependencies": {}
}
